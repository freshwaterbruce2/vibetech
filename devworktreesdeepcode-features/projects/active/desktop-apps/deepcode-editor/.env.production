# Production Environment Configuration for DeepCode Editor

# DeepSeek AI Configuration
VITE_DEEPSEEK_API_KEY=your_production_api_key_here
VITE_DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# Application Settings
VITE_APP_NAME=DeepCode Editor
VITE_APP_VERSION=1.0.0
VITE_APP_BUILD=production

# Feature Flags
VITE_ENABLE_TELEMETRY=true
VITE_ENABLE_AUTO_UPDATE=true
VITE_ENABLE_CRASH_REPORTING=true
VITE_ENABLE_ANALYTICS=true

# API Endpoints
VITE_UPDATE_SERVER_URL=https://updates.deepcode-editor.com
VITE_TELEMETRY_URL=https://telemetry.deepcode-editor.com
VITE_ERROR_REPORTING_URL=https://errors.deepcode-editor.com

# Security Settings
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=https://api.deepseek.com,https://updates.deepcode-editor.com

# Performance Settings
VITE_ENABLE_CODE_SPLITTING=true
VITE_ENABLE_WORKER_THREADS=true
VITE_MAX_WORKER_THREADS=4

# Storage Settings
VITE_LOCAL_STORAGE_PREFIX=deepcode_
VITE_SESSION_TIMEOUT_MINUTES=30

# Debug Settings (set to false in production)
VITE_DEBUG_MODE=false
VITE_SHOW_DEV_TOOLS=false