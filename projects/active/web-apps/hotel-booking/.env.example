# Vibe Bookings Environment Variables Example
# Copy this file to .env and fill in your actual values

# Application Configuration
REACT_APP_NAME=Vibe Bookings
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001
REACT_APP_API_TIMEOUT=30000

# Hotel Booking APIs
REACT_APP_LITEAPI_BASE_URL=https://api.liteapi.travel/v1
REACT_APP_LITEAPI_API_KEY=your_liteapi_key_here

# Authentication
REACT_APP_AUTH_DOMAIN=your_auth_domain
REACT_APP_AUTH_CLIENT_ID=your_auth_client_id
REACT_APP_JWT_SECRET=your_jwt_secret_here

# Payment Processing (Square)
REACT_APP_SQUARE_APPLICATION_ID=your_square_app_id
REACT_APP_SQUARE_LOCATION_ID=your_square_location_id
REACT_APP_SQUARE_ENVIRONMENT=sandbox

# Map Integration
REACT_APP_MAPBOX_ACCESS_TOKEN=your_mapbox_token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT_SUPPORT=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true

# Monitoring & Analytics
REACT_APP_SENTRY_DSN=your_sentry_dsn
REACT_APP_GTM_ID=your_google_tag_manager_id

# Third-Party Services
REACT_APP_RECAPTCHA_SITE_KEY=your_recaptcha_site_key

# Development Tools
REACT_APP_ENABLE_DEV_TOOLS=true
REACT_APP_LOG_LEVEL=info