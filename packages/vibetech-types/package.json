{
  "name": "@vibetech/types",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions for VibeTech monorepo projects",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tasks": {
      "import": "./dist/tasks.js",
      "types": "./dist/tasks.d.ts"
    },
    "./errorfix": {
      "import": "./dist/errorfix.js",
      "types": "./dist/errorfix.d.ts"
    },
    "./multifile": {
      "import": "./dist/multifile.js",
      "types": "./dist/multifile.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "types",
    "monorepo",
    "shared",
    "tasks",
    "errors",
    "files"
  ],
  "author": "VibeTech",
  "license": "MIT"
}
