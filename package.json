{
  "name": "@vibetech/workspace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Vibe Tech multi-project monorepo with booking platforms, web apps, and desktop applications",
  "packageManager": "pnpm@9.15.0",
  "workspaces": [
    "backend",
    "packages/*",
    "projects/active/web-apps/*",
    "projects/active/desktop-apps/*",
    "projects/Vibe-Subscription-Guard"
  ],
  "resolutions": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --config vite.config.production.ts",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "analyze": "npm run build:production && npx vite-bundle-analyzer dist",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:debug": "playwright test --debug",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:coverage": "npm run test:unit:coverage && npm run crypto:coverage",
    "test:coverage:all": "nx run-many -t test:coverage",
    "coverage:report": "npm run test:unit:coverage && npm run crypto:coverage:report",
    "typecheck": "tsc --noEmit",
    "quality": "nx run-many -t quality",
    "quality:fix": "nx run-many -t quality:fix",
    "quality:affected": "nx affected -t quality",
    "build:all": "nx run-many -t build",
    "build:production:all": "nx run-many -t build:production",
    "test:all": "nx run-many -t test",
    "test:unit:all": "nx run-many -t test:unit",
    "lint:all": "nx run-many -t lint",
    "lint:fix:all": "nx run-many -t lint:fix",
    "typecheck:all": "nx run-many -t typecheck",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph",
    "nx:reset": "nx reset",
    "workspace:install": "npm install && npm run projects:install",
    "workspace:clean": "npm run clean:root && npm run projects:clean",
    "projects:install": "npm run crypto:install",
    "projects:clean": "npm run crypto:clean && npm run clean:artifacts",
    "crypto:install": "cd projects/crypto-enhanced && python -m venv .venv && .venv\\Scripts\\pip install -r requirements.txt",
    "crypto:clean": "cd projects/crypto-enhanced && rmdir /s /q .venv __pycache__ 2>nul || echo \"Clean completed\"",
    "clean:root": "rmdir /s /q node_modules .nx dist playwright-report 2>nul || echo \"Root clean completed\"",
    "clean:nx": "nx run-many -t clean",
    "clean:artifacts": "del /q *.log 2>nul && del /q fix-*.* 2>nul || echo \"Artifact clean completed\"",
    "monorepo:health": "npm run quality && npm run crypto:test",
    "crypto:test": "cd projects/crypto-enhanced && .venv\\Scripts\\python.exe run_tests.py",
    "crypto:coverage": "cd projects/crypto-enhanced && .venv\\Scripts\\python.exe -m pytest --cov=. --cov-report=term-missing --cov-report=html --cov-report=json --cov-report=xml",
    "crypto:coverage:report": "cd projects/crypto-enhanced && start coverage_html\\index.html",
    "parallel": "powershell -ExecutionPolicy Bypass -File scripts\\workspace-manager.ps1 -Action parallel",
    "parallel:dev": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group dev",
    "parallel:full-stack": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group full-stack",
    "parallel:trading": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group trading",
    "parallel:desktop": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group desktop",
    "parallel:web-apps": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group web-apps",
    "parallel:test": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group testing",
    "parallel:watch": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group dev -Watch",
    "parallel:dashboard": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group dev -Dashboard",
    "parallel:status": "powershell -ExecutionPolicy Bypass -File scripts\\workspace-manager.ps1 -Action status -Parallel",
    "parallel:stop": "powershell -ExecutionPolicy Bypass -Command \"& { . .\\scripts\\workspace-manager.ps1 -Action status; Stop-ParallelEnvironment }\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@tanstack/react-query": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.22",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.545.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.64.0",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^7.9.3",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.180.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@nx/js": "^21.6.3",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.0",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "lovable-tagger": "^1.1.10",
    "nx": "21.6.3",
    "postcss": "^8.5.6",
    "puppeteer": "^24.23.0",
    "tailwindcss": "^3.4.15",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
