{
  "name": "vibe-tech-lovable",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --config vite.config.production.ts",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "analyze": "npm run build:production && npx vite-bundle-analyzer dist",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:debug": "playwright test --debug",
    "typecheck": "tsc --noEmit",
    "quality": "npm run lint && npm run typecheck && npm run build",
    "quality:fix": "npm run lint:fix && npm run typecheck",
    "workspace:install": "npm install && npm run projects:install",
    "workspace:clean": "npm run clean:root && npm run projects:clean",
    "projects:install": "npm run crypto:install",
    "projects:clean": "npm run crypto:clean && npm run clean:artifacts",
    "crypto:install": "cd projects/crypto-enhanced && python -m venv .venv && .venv\\Scripts\\pip install -r requirements.txt",
    "crypto:clean": "cd projects/crypto-enhanced && rmdir /s /q .venv __pycache__ 2>nul || echo \"Clean completed\"",
    "clean:root": "rmdir /s /q node_modules .nx .turbo dist playwright-report 2>nul || echo \"Root clean completed\"",
    "clean:artifacts": "del /q *.log 2>nul && del /q fix-*.* 2>nul || echo \"Artifact clean completed\"",
    "monorepo:health": "npm run quality && npm run crypto:test",
    "crypto:test": "cd projects/crypto-enhanced && .venv\\Scripts\\python.exe run_tests.py",
    "parallel": "powershell -ExecutionPolicy Bypass -File scripts\\workspace-manager.ps1 -Action parallel",
    "parallel:dev": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group dev",
    "parallel:full-stack": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group full-stack",
    "parallel:trading": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group trading",
    "parallel:desktop": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group desktop",
    "parallel:web-apps": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group web-apps",
    "parallel:test": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group testing",
    "parallel:watch": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group dev -Watch",
    "parallel:dashboard": "powershell -ExecutionPolicy Bypass -File scripts\\Start-ParallelMonorepo.ps1 -Group dev -Dashboard",
    "parallel:status": "powershell -ExecutionPolicy Bypass -File scripts\\workspace-manager.ps1 -Action status -Parallel",
    "parallel:stop": "powershell -ExecutionPolicy Bypass -Command \"& { . .\\scripts\\workspace-manager.ps1 -Action status; Stop-ParallelEnvironment }\""
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@tanstack/react-query": "^5.56.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.3.0",
    "framer-motion": "^11.18.2",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.162.0",
    "vaul": "^0.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.5.5",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.4.47",
    "puppeteer": "^24.17.0",
    "tailwindcss": "^3.4.11",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^7.1.4"
  }
}
